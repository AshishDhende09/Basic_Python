    "import math\n",
    "def circle(rad):\n",
    "    if type(rad) not in [int,float]:\n",
    "        raise TypeError(\"invalid input\")\n",
    "    if rad<0:\n",
    "        raise ValueError(\"-ve number does not exists\")\n",
    "        \n",
    "    return math.pi*rad*rad"
